<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
	
	<gwt:DockLayoutPanel unit="PX">
		<gwt:north size="50">
			<gwt:HTMLPanel styleName="b-app-header">
				Customer Relationship Management(CRM) 0.0.b
			</gwt:HTMLPanel>
		</gwt:north>	
		
		<gwt:west size="200">
			<gwt:HTMLPanel styleName="b-app-navigator">
				<a id="contragent-view" href="#contragent:">Contragent</a>
				<a id="contact-view" href="#contact:">Contacts</a>
				<a id="contact-type-view" href="#contact_type:">Contact type</a>
				<a id="user-view" href="#user:">User</a>
			</gwt:HTMLPanel>
		</gwt:west>
		<gwt:center>
			<gwt:SimplePanel ui:field="appContent" width="" height="" styleName="b-app-content"/>
		</gwt:center>
	</gwt:DockLayoutPanel>
	
</ui:UiBinder>
 